@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix cogsgit: <https://github.com/GSS-Cogs/> .
@prefix cogs: <http://gss-cogs.uk/def/arch/> .

<http://gss-cogs.uk/def/arch> a owl:Ontology;
  rdfs:label "COGS Architecture" ;
.

cogs:vis a owl:AnnotationProperty ;
  rdfs:label "visualise as" ;
.

cogs:style a owl:Annotation ;
  rdfs:label "style" ;
.

cogs:container a owl:Annotation ;
  rdfs:label "contained by" ;
.

cogs:node a owl:Thing .
cogs:edge a owl:Thing .

cogs:Process a owl:Class ;
  rdfs:label "Process" ;
.

cogs:action a owl:ObjectProperty ;
  rdfs:label "Action" ;
.

cogs:Resource a owl:Class ;
  rdfs:label "Resource" ;
.

cogs:Container a owl:Class ;
  rdfs:label "Container" ;
.

cogs:Library a owl:Class ;
  rdfs:label "Library" ;
.

:PMD a cogs:Container ;
  rdfs:label "Publish my Data" ;
  cogs:vis cogs:node ;
  cogs:style "fill: #dddddd; stroke: #999" ;
.

:table2qb a cogs:Container ;
  rdfs:label "table2qb" ;
  cogs:container :PMD ;
  cogs:vis cogs:node ;
  cogs:style "fill: #bbbbbb; stroke: #999;" ;
.

cogs:generates a owl:ObjectProperty ;
  rdfs:subPropertyOf cogs:action ;
  rdfs:label "generates" ;
  cogs:vis cogs:edge ;
.

cogs:input_to a owl:ObjectProperty ;
  rdfs:subPropertyOf cogs:action ;
  rdfs:label "input" ;
  cogs:vis cogs:edge;
.

cogs:refers_to a owl:ObjectProperty ;
  rdfs:subPropertyOf cogs:action ;
  rdfs:label "refers to" ;
  cogs:vis cogs:edge ;
.

cogs:fetches a owl:ObjectProperty ;
  rdfs:subPropertyOf cogs:action ;
  rdfs:label "fetches" ;
  cogs:vis cogs:edge ;
.

cogs:uses a owl:ObjectProperty ;
  rdfs:subPropertyOf cogs:action ;
  rdfs:label "uses" ;
  cogs:vis cogs:edge ;
.

cogs:scrapes a owl:ObjectProperty ;
  rdfs:subPropertyOf cogs:action ;
  rdfs:label "scrapes" ;
  cogs:vis cogs:edge ;
.

###########################################################################################################

cogsgit:top-level a cogs:LandingPage ;
  rdfs:label "Statutory Homelessness detailed Local Authority-Level" ;
  rdfs:comment "https://www.gov.uk/government/statistical-data-sets/live-tables-on-homelessness";
  cogs:generates cogsgit:sheet1, cogsgit:sheet2, cogsgit:sheet3, cogsgit:sheet4 ;
.
####
cogsgit:sheet1 a cogs:Sheets ;
  rdfs:label "" ;
  rdfs:comment "A1, A2, A3, A4, A5P, A5R, A7, A11" ;
  cogs:requires cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "" ;
  rdfs:comment "P1, P2, R1, R2" ;
  cogs:requires cogsgit:dataset2 ;
.
cogsgit:sheet3 a cogs:Sheets ;
  rdfs:label "" ;
  rdfs:comment "MD1, MD2, MD3" ;
  cogs:requires cogsgit:dataset3 ;
.
cogsgit:sheet4 a cogs:Sheets ;
  rdfs:label "" ;
  rdfs:comment "TA1, TA2" ;
  cogs:requires cogsgit:dataset4 ;
.
####
cogsgit:dataset1 a cogs:Datasets ;
  rdfs:label "" ;
  rdfs:comment "MHCLG Homelessness - Households initially assessed as threatened with homelessness (owed prevention duty) or homeless (owed relief duty)" ;
  cogs:requires cogsgit:columns1 ;
.
cogsgit:dataset2 a cogs:Datasets ;
  rdfs:label "" ;
  rdfs:comment "MHCLG Homelessness - Households whose prevention or relief duty ended by reason and accommodation type" ;
  cogs:requires cogsgit:columns2 ;
.
cogsgit:dataset3 a cogs:Datasets ;
  rdfs:label "" ;
  rdfs:comment "MHCLG - Households assessed, following relief duty end, as unintentionally homeless and priority need (owed main duty)" ;
  cogs:requires cogsgit:columns3 ;
.
cogsgit:dataset4 a cogs:Datasets ;
  rdfs:label "" ;
  rdfs:comment "MHCLG Homelessness - Households in temporary accommodation by household composition and accommodation type" ;
  cogs:requires cogsgit:columns4 ;
.
####
cogsgit:columns1 a cogs:Columns ;
  rdfs:label "" ;
  rdfs:comment "Period, Accommodation Type, Household Composition, Initial Assessment, Reason for loss or threat of loss of home, Support needs of household, Duty Owed, Referral Public Body, Eligibility for Homelessness Status" ;
.
cogsgit:columns2 a cogs:Columns ;
  rdfs:label "" ;
  rdfs:comment "Period, ONS Geography Code, Accommodation Type, Reason duty Ended, Duty Type" ;
.
cogsgit:columns3 a cogs:Columns ;
  rdfs:label "" ;
  rdfs:comment "Period, ONS Geography Code, Decision on Duty Owed, Reason duty Ended, Priority Need" ;
.
cogsgit:columns4 a cogs:Columns ;
  rdfs:label "" ;
  rdfs:comment "Period, ONS Geography Code, Accommodation Type, Household Composition" ;
.
###################################################################################################
